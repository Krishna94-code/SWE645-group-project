stage('Checkout') { 
       checkout scm
       withAnt(installation: 'LocalAnt') {
       sh "ant build"
}
    }
    

    stage('Build') { 
        echo "Building in progress...."
    }
    stage('Test') { 
        echo "Testing in progress...."
    }
    stage('Deploy') { 
        echo "Deployment in progress...."
    }
    

